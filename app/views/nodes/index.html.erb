<h1>Listing nodes</h1>

<table>
  <tr>
  </tr>

<% for node in @nodes %>
  <tr>
    <td><%= link_to 'Show', node %></td>
    <td><%= link_to 'Edit', edit_node_path(node) %></td>
    <td><%= link_to 'Destroy', node, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

